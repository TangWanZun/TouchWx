.carList {
  padding: 110px 15px 20px 15px;
}
.carList .carList-tab {
  position: fixed;
  left: 0;
  top: 50px;
  display: flex;
  justify-content: flex-start;
  width: 100vw;
  height: 50px;
  padding-top: 10px;
  opacity: 0.95;
}
.carList .carList-tab > div {
  width: 50%;
  text-align: center;
  color: white;
  padding-bottom: 10px;
  transition: border-bottom 0.2s;
}
.carList .carList-tab > .carList-tab-show {
  border-bottom: 3px solid #3be9f2;
}
.carList .coupon-car {
  position: relative;
  margin-bottom: 15px;
  border-radius: 5px;
  font-size: 20px;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  color: black;
  overflow: hidden;
  border-left: 5px solid #3be9f2;
  background-color: white;
  transition: background-color 0.1s;
}
.carList .coupon-car:active {
  background-color: rgba(255, 255, 255, 0.9);
}
.carList .coupon-car .coupon-type {
  position: absolute;
  height: 120px;
  width: 120px;
  right: -35px;
  top: -35px;
}
.carList .coupon-car .coupon-type > img {
  width: 100%;
  height: 100%;
}
.carList .coupon-car .coupon-car-center {
  font-size: 17px;
  padding-right: 15px;
  text-align: left;
  display: flex;
  justify-content: flex-start;
}
.carList .coupon-car .coupon-car-center .coupon-car-center-money {
  font-size: 18px;
  display: flex;
  align-items: center;
  justify-content: center;
  min-width: 80px;
  flex-shrink: 0;
  border-right: 1px dotted rgba(0, 0, 0, 0.5);
  flex-grow: 0;
  padding: 0 3px;
}
.carList .coupon-car .coupon-car-center .coupon-car-center-date {
  flex-grow: 1;
}
.carList .coupon-car .coupon-car-center .coupon-car-center-date .coupon-car-center-name {
  font-size: 17px;
  text-align: center;
  padding: 5px 0;
}
.carList .coupon-car .coupon-car-center .coupon-car-center-date .coupon-car-center-flex {
  display: flex;
  justify-content: space-between;
}
.carList .coupon-car .coupon-car-center .coupon-car-center-date .coupon-car-center-exp {
  padding-left: 5px;
  color: rgba(0, 0, 0, 0.5);
  font-size: 15px;
}
.carList .coupon-car .coupon-car-center .coupon-car-center-date .coupon-car-center-exp:last-child {
  margin-bottom: 10px;
}
.carList .coupon-car .coupon-sandwich {
  max-height: 0px;
  transition: max-height 0.3s;
  border-top: 1px solid rgba(0, 0, 0, 0.5);
  overflow: auto;
}
.carList .coupon-car .coupon-sandwich-show {
  max-height: 200px;
}
.carList .coupon-car .coupon-car-botton {
  background-color: rgba(0, 0, 0, 0.5);
  text-align: right;
  padding: 5px 10px;
  color: rgba(255, 255, 255, 0.8);
  display: flex;
  justify-content: space-between;
}
.carList .coupon-car .coupon-car-botton > div {
  font-size: 13px;
}
