/* pages/client/clientDetails/coupon.wxss */
.datalist{
        padding:0 20px;
        padding-top: 60px;
        min-height: 100vh;
        background-color: #f9faf7;
}
/* 保证核销按钮不会遮挡页面 */
.foot{
        height: 10vw;
}
/* 顶部栏 */
.page-tap{
        display: flex;
        justify-content: flex-start;
        align-items: center;
        background-color: white;
        position: fixed;
        top:0px;
        left:0;
        right:0;
}
.page-tap-item{
        text-align: center;
        flex-grow: 1;
        font-size: 13px;
        border-bottom:1px solid rgba(0, 0, 0, 0.1);
        transition: border-bottom 0.2s;
        box-sizing: border-box;
        height: 40px;
        line-height: 40px;
}
.page-tap-item-show{
        border-bottom:3px solid #4eb4ff;
}
.page-blod-head {
  display: flex;
  flex-direction: column;
  height: 100%;
  width: 10px;
  overflow: hidden;
}
.page-blod-head .page-grow {
  flex-grow: 1;
  background-color: #ffbe32;
}
.page-blod-head .page-rauid {
  width: 20px;
  height: 20px;
  background-image: radial-gradient(circle, rgba(255, 255, 255, 0) 0%, rgba(255, 255, 255, 0) 70%, #ffbe32 0%);
}
.page-blod-head .page-rauid:first-child {
  margin-top: -10px;
}
.page-blod-head .page-rauid:last-child {
  margin-bottom: -10px;
}
.page-blod-foot {
  transform: rotate(180deg);
  display: flex;
  flex-direction: column;
  height: 100%;
  width: 10px;
  overflow: hidden;
}
.page-blod-foot .page-grow {
  flex-grow: 1;
  background-color: #ffffff;
}
.page-blod-foot .page-rauid {
  width: 20px;
  height: 20px;
  background-image: radial-gradient(circle, rgba(255, 255, 255, 0) 0%, rgba(255, 255, 255, 0) 70%, #ffffff 0%);
}
.page-blod-foot .page-rauid:first-child {
  margin-top: -10px;
}
.page-blod-foot .page-rauid:last-child {
  margin-bottom: -10px;
}
.item {
  margin-bottom: 20px;
}
.item .item-content {
  min-height: 100px;
  display: flex;
}
.item .item-left {
  flex-shrink: 0;
  width: 90px;
  background-color: #ffbe32;
  border-radius: 10px 0 0 10px;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  font-size: 18px;
  font-weight: bold;
  color: white;
}
.item .item-left .item-left-name {
  font-size: 15px;
  margin-bottom: 10px;
}
.item .item-left .item-gqdata {
  margin-top: 5px;
  color: #FD604C;
  font-size: 12px;
  border: 1px solid #FD604C;
  padding: 2px 2px;
}
.item .item-blod {
  display: flex;
  flex-shrink: 0;
}
.item .item-right {
  flex-grow: 1;
  border-radius: 0 10px 10px 0;
  background-color: #ffffff;
  padding: 10px;
  position: relative;
}
.item .item-right .item-name {
  font-size: 18px;
}
.item .item-right .item-date {
  font-size: 13px;
  color: rgba(0, 0, 0, 0.5);
  margin-top: 3px;
}
.item .item-right .item-flex {
  display: flex;
  justify-content: space-between;
  margin: 3px 0;
}
.item .item-right .item-text {
  color: rgba(0, 0, 0, 0.5);
  font-size: 13px;
}
.item .item-right .item-gz {
  text-align: right;
  font-size: 12px;
  text-decoration: underline;
  color: #4ab5f4;
  margin-top: -5px;
}
.item .item-sandwich {
  margin-top: 5px;
  color: #707070;
  max-height: 0px;
  transition: max-height 0.3s;
  overflow: hidden;
  position: relative;
  margin-top: -5px;
}
.item .item-sandwich::before {
  content: "";
  height: 10px;
  display: block;
}
.item .item-sandwich::after {
  content: "";
  position: absolute;
  right: 20px;
  top: 4px;
  width: 12px;
  height: 12px;
  background-color: #eaf7ff;
  border-top: 1px solid #4ab5f4;
  border-left: 1px solid #4ab5f4;
  transform: rotate(45deg);
}
.item .item-sandwich .item-sandwich-content {
  background-color: #eaf7ff;
  border: 1px solid #4ab5f4;
  border-radius: 10px;
  overflow: auto;
  max-height: 200px;
  padding: 10px;
}
.item .item-sandwich .item-sandwich-content .item-sandwich-title {
  font-size: 15px;
}
.item .item-sandwich .item-sandwich-content .item-sandwich-text {
  font-size: 15px;
  white-space: pre-wrap;
  word-wrap: break-word;
}
.item .item-sandwich-show {
  max-height: 200px;
}
