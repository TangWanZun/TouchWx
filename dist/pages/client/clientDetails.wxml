<view class='body'>
  <!-- 头部 -->
  <view class='header'>
    <!-- 头部内容 -->
    <view class='header-center'>
      <view class='header-top'>
        <!-- 头像 -->
        <view class='header-img'>
          <image  src='{{formData.ProfilePhoto?imgUrl+formData.ProfilePhoto:"/assets/chat/kh_tx.svg"}}'></image>
        </view>
        <!-- 爱好标签 -->
        <view class='header-likes-box'>
          <view class='header-likes'>
            <view class='header-likes-item'wx:if="{{index<10}}" wx:for="{{labelList}}" wx:key="{{index}}">{{item.Name}}</view>
          </view>
        </view>
      </view>
      <view class='header-botton'>
        <view class='header-botton-item'>
          <view class='header-botton-item-title'>储值</view>
          <view class='header-botton-item-number'>￥{{formData.AvailableBalance||'-'}}</view>
        </view>
        <view class='header-botton-item'>
          <view class='header-botton-item-title'>积分</view>
          <view class='header-botton-item-number'>{{formData.AvailableScore||'-'}}</view>
        </view>
        <view class='header-botton-item'>
          <view class='header-botton-item-title'>电子券</view>
          <view class='header-botton-item-number'>{{formData.AvailableECard||'-'}}张</view>
        </view>
        <!-- <view class='header-botton-item'>
          <view class='header-botton-item-title'>电子券</view>
          <view class='header-botton-item-number'>{{clientData.coupon}}张</view>
        </view> -->
      </view>
    </view>
    <!-- 背景图 -->
    <view class='header-bg'>
      <image mode='aspectFill'  src='{{formData.ProfilePhoto?imgUrl+formData.ProfilePhoto:"/assets/chat/kh_tx.svg"}}'></image>
    </view>
  </view>
  <!-- 正文内容 -->
  <view class='content'>
    <view class='content-card'>
      <view class='content-card-title'>
        <view class='content-card-title-left'>联系方式</view>
        <!-- <view class='content-card-title-right'>更改联系方式</view> -->
      </view>
      <view class='content-card-body'>
        <view class='panel-item'>
          <!-- 拨打电话 -->
          <view class='panel' >
            <view class='panel-number'>{{formData.Phone||'-'}}</view>
            <view class='panel-icon' data-panel="{{formData.Phone}}" catchtap="makePhoneCall"><image src='/assets/icon/call.svg'></image></view>
          </view>
          <!-- 微信 -->
          <view class='panel'>
            <view class='panel-number'>{{formData.NickName}}</view>
            <navigator  url='/pages/message/chat?openId={{formData.OpenId}}&cardName={{formData.CardName}}&wxId={{formData.WxId}}&headImg={{formData.ProfilePhoto}}' class='panel-icon'><image src='/assets/icon/WX.svg'></image></navigator>
          </view>
        </view>
      </view>
    </view>
