    <view class='content-card' style='padding-bottom:40px'>
      <view class='content-card-title'>
        <view class='content-card-title-left'>用户信息</view>
        <!-- <view class='content-card-title-right'>保存修改</view> -->
      </view>
      <view class='content-card-body'>
        <!-- 性别 -->
        <!-- <picker mode="selector" data-key="userInfor.Sex" data-arr="{{sexArr}}" range="{{sexArr}}" bindchange="inputCall" > -->
          <view class='client'>
              <view class='client-header' >
                <view class='client-title'>性别</view>
              </view>
              <view class='client-value'>{{userInfor.Sex}}</view>
              <!-- <view class='client-right'><image src='/assets/icon/right.svg'></image></view> -->
          </view>
        <!-- </picker> -->
        <!-- 出生日 -->
        <!-- <picker mode="date" data-key="userInfor.BirthDate" value="{{userInfor.BirthDate}}"  bindchange="inputCall"> -->
          <view class='client'>
              <view class='client-header'>
                <view class='client-title'>出生日</view>
              </view>
              <view class='client-value'>{{userInfor.BirthDate}}</view>
              <!-- <view class='client-right'><image src='/assets/icon/right.svg'></image></view> -->
          </view>
        <!-- </picker> -->
        <!-- 学历 -->
        <!-- <picker mode="selector" data-key="education.value" data-arr="{{education}}" range="{{education.list}}" range-key='Name' bindchange="inputCall" > -->
          <view class='client'>
              <view class='client-header' >
                <view class='client-title'>学历</view>
              </view>
              <view class='client-value'>{{userInfor.EducationCode}}</view>
              <!-- <view class='client-right'><image src='/assets/icon/right.svg'></image></view> -->
          </view>
        <!-- </picker> -->
        <view class='client'>
            <view class='client-header' >
              <view class='client-title'>职业</view>
            </view>
            <view class='client-value'><input disabled="true"  data-key="userInfor.Profession" placeholder='未填写' value='{{userInfor.Profession}}' cursor-spacing="30" bindinput='inputCall'></input></view>
        </view>
        <view class='client'>
            <view class='client-header' >
              <view class='client-title'>证件号</view>
            </view>
            <view class='client-value'><input disabled="true"  data-key="userInfor.IDCard" placeholder='未填写' value='{{userInfor.IDCard}}' cursor-spacing="30" type="idcard" bindinput='inputCall'></input></view>
        </view>
        <view class='client'>
            <view class='client-header' >
              <view class='client-title'>住址</view>
            </view>
            <view class='client-value'><input disabled="true"  data-key="userInfor.AddressDetails" placeholder='未填写' value='{{userInfor.AddressDetails}}' cursor-spacing="30" bindinput='inputCall'></input></view>
        </view>
        <!-- 婚姻 -->
        <!-- <picker mode="selector" data-key="userInfor.marriage.value" range="{{userInfor.marriage.list}}" value="{{item.value}}" bindchange="inputCall" > -->
          <!-- <view class='client'>
              <view class='client-header' >
                <view class='client-title'>婚姻状况</view>
              </view>
              <view class='client-value'>{{userInfor.marriage.list[userInfor.marriage.value]}}</view> -->
              <!-- <view class='client-right'><image src='/assets/icon/right.svg'></image></view> -->
          <!-- </view> -->
        <!-- </picker> -->
        <!-- 子女情况 -->
        <view class='client'>
            <view class='client-header' >
              <view class='client-title'>子女情况</view>
            </view>
            <view class='client-value'><input disabled="true"  data-key="userInfor.ChildrenDesc" placeholder='未填写' value='{{userInfor.ChildrenDesc}}'  cursor-spacing="30" bindinput='inputCall'></input></view>
        </view>
        <!-- 子女数量-->
        <view class='client'>
            <view class='client-header' >
              <view class='client-title'>子女数量</view>
            </view>
            <view class='client-value'><input disabled="true" data-key="userInfor.ChildrenCount" placeholder='未填写' value='{{userInfor.ChildrenCount}}'  type="number" cursor-spacing="30" bindinput='inputCall'></input></view>
        </view>
        <!--常去商圈 -->
        <view class='client'>
            <view class='client-header' >
              <view class='client-title'>常去商圈</view>
            </view>
            <view class='client-value'><input disabled="true"  data-key="userInfor.OftenGoto" placeholder='未填写' value='{{userInfor.OftenGoto}}'  cursor-spacing="30" bindinput='inputCall'></input></view>
        </view>
      </view>
    </view>
  </view>
</view>
