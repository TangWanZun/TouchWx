<view class="page-body">
  <view class="userCard app-shadow-2">
    <view class="userCard-left">
      <view class="userCard-name">
        <view  class="userCard-name-text">{{loginInfo.UserName}}</view>
        <!-- <navigator url='../user/myQr'><image src='/assets/icon/qr.svg'></image></navigator> -->
      </view>
      <view>
        <view class="userCard-section">{{loginInfo.DeptName}}</view>
        <view class="userCard-company">{{loginInfo.CmpName}}</view>
      </view>
    </view>
    <view class="userCard-right">
      <view class="userCard-img">
        <head-img></head-img>
      </view>
      <view class="userCard-index">{{loginInfo.UserCode}}</view>
    </view>
  </view>
  <view class="my-data">
    <navigator url="../user/myData" class="my-data-title">
      <view>我的数据</view>
      <view class="my-data-title-icon"><image src='/assets/icon/right.svg'></image></view>
    </navigator>
    <view class='my-data-con'>
      <view class="ui-row" style="">
        <view class="ui-col ui-col-6" style="flex: 0 0 50%;">
          <view class="ui-col-content">
            <view class="my-data-item-title">总产值
              <span>（元）</span>
            </view>
            <view class="my-data-item-num">1562000</view>
          </view>
        </view>
        <view class="ui-col ui-col-6" style="flex: 0 0 50%;">
          <view class="ui-col-content">
            <view class="my-data-rangking">
              <view class='my-data-rangking-in'>
                <view class="my-data-item-title">本月店内排名</view>
                <view class="my-data-item-index">第<span>1</span>名</view></view>
              </view>
              <view class="my-data-img">
                <head-img></head-img>
              </view>
          </view>
        </view>
      </view>
    </view>
    <view class='my-data-list app-item-div app-scroll-none'>
      <view  class="my-data-item app-shadow-2">
          <view class="my-data-item-title my-data-item-list">本月目标
            <span>（台）</span>
          </view>
          <view class="my-data-item-num my-data-item-list">3<span>/5</span></view>
      </view>
      <view class="my-data-item  app-shadow-2">
          <view class="my-data-item-title my-data-item-list">我的客户
            <span>（位）</span>
          </view>
          <view class="my-data-item-num my-data-item-list">103</view>
      </view>
      <view  class="my-data-item  app-shadow-2">
        <view class="my-data-item-title my-data-item-list"> 活跃客户
          <span>（位）</span>
        </view>
        <view class="my-data-item-num my-data-item-list">32</view>
      </view>
      <view  class="my-data-item  app-shadow-2">
        <view class="my-data-item-title my-data-item-list"> 当日接车
          <span>（台）</span>
        </view>
        <view class="my-data-item-num my-data-item-list">3</view>
      </view>
      <view  class="my-data-item  app-shadow-2">
        <view class="my-data-item-title my-data-item-list"> 当月产值
          <span>（元）</span>
        </view>
        <view class="my-data-item-num my-data-item-list">90000</view>
      </view>
    </view>
    <navigator url='/pages/tabBar/login' class="my-data-title">
      <view>注销登陆</view>
      <view class="my-data-title-icon"><image src='/assets/icon/right.svg'></image></view>
    </navigator>
  </view>
</view>