<view class="page-body">
  <view class='page-list'>
    <navigator url="/pages/message/chat?openId={{item.OpenId}}" class='page-item' wx:for="{{dataList}}" wx:key="{{item.id}}">
      <view class='page-item-img'><image src='{{item.ProfilePhoto?imgUrl+item.ProfilePhoto:"/assets/chat/kh_tx.svg"}}'></image><view wx:if="{{item.PendCount!==0}}" class='page-item-badge'>{{item.PendCount}}</view></view>
      <view class='page-item-con'>
        <view class='page-item-con-con'>
          <view class='page-item-name'>{{item.CardName?item.CardName:item.NickName}}</view>
          <view class='page-item-date'>{{item.CreateDate}}</view>
        </view>
        <view class='page-item-message' wx:if="{{item.MsgType==CHAT_TYPE.CHAT_TEXT}}">{{item.MsgData}}</view>
        <view class='page-item-message' wx:elif="{{item.MsgType==CHAT_TYPE.CHAT_IAMGE}}">[图片]</view>
        <view class='page-item-message' wx:elif="{{item.MsgType==CHAT_TYPE.CHAT_ADDS}}">[位置]</view>
        <view class='page-item-message' wx:elif="{{item.MsgType==CHAT_TYPE.CHAT_VOICE}}">[语音]</view>
      </view>
    </navigator>
    <view class="list-loging" wx:if="{{pageOver}}">数据加载完成</view>
    <view class="list-loging" wx:else><view>数据加载中</view><image src='/assets/icon/jzz.svg'></image></view>
  </view>
</view>