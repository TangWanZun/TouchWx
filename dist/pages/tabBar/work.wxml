<view>
  <view class='work-type-list app-scroll-none'>
    <navigator class='work-type' wx:for="{{workTypeData}}" wx:key="{{item.title}}">
      <view class='work-type-icon'><image src='{{item.iconUrl}}'></image></view>
      <view  class='work-type-title'>{{item.title}}</view>
    </navigator>
  </view>
  <view class='work-list-title'>
    今日工作
  </view>
  <view class='work-list'>
    <view class='work-item-box' wx:for="{{dataList}}" wx:key="{{item.id}}">
      <!-- 救援服务 -->
      <navigator wx:if="{{item.workType === workType.WORK_JY}}"  class='work-item app-shadow-3 work-jyfw'>
        <view class='work-item-head'>
          紧急救援
        </view>
        <view class='work-item-body'>
          <view class='work-item-body-title'>
            <view class='work-item-headimg'><image src='{{item.clientImg}}'></image></view>
            <view class='work-item-name'>{{item.clientName}}</view>
          </view>
          <view class='work-item-car'>{{item.data.carNum}}</view>
          <view class='work-item-body-con'>
            <span><image src='/assets/icon/dz.svg'></image></span>{{item.data.adds}}
          </view>
        </view>
        <view class='work-item-foot'>
          {{item.date}}
        </view>
      </navigator>
      <!-- 投诉建议 -->
      <navigator wx:elif="{{item.workType === workType.WORK_TS}}" class='work-item app-shadow-3 work-tsjy'>
        <view class='work-item-head'>
          投诉建议
        </view>
        <view class='work-item-body'>
          <view class='work-item-body-title'>
            <view class='work-item-headimg'><image src='{{item.clientImg}}'></image></view>
            <view class='work-item-name'>{{item.clientName}}</view>
          </view>
          <view class='work-item-body-con'>
            {{item.data.conText}}
          </view>
        </view>
        <view class='work-item-foot'>
          {{item.date}}
        </view>
      </navigator>
      <!-- 服务预约 -->
      <navigator wx:elif="{{item.workType === workType.WORK_YY}}" class='work-item app-shadow-3 work-fwyy'>
        <view class='work-item-head'>
          {{item.data.makeType}}
        </view>
        <view class='work-item-body'>
          <view class='work-item-body-con'>
            <view class='work-item-headimg'><image src='{{item.clientImg}}'></image></view>
            <view class='work-item-name'>{{item.clientName}}</view>
          </view>
          <view class='work-item-body-title'>
            {{item.data.carNum}}
          </view>
        </view>
        <view class='work-item-foot'>
          <view>{{item.date}}</view>
          <view>{{item.data.time}}</view>
        </view>
      </navigator>
      <!-- 活动报名 -->
      <navigator wx:elif="{{item.workType === workType.WORK_HD}}" class='work-item app-shadow-3 work-hdbm'>
        <view class='work-item-head'>
          <view class='work-item-title'>{{item.data.assName}}</view>
          <view class='work-item-bg'><image mode="aspectFill" src='{{item.data.assImg}}'></image></view>
        </view>
        <view class='work-item-body'>
          <view class='work-item-body-title'>活动报名</view>
          <view class='work-item-body-con'>
            <view class='work-item-headimg'><image src='{{item.clientImg}}'></image></view>
            <view class='work-item-name'>{{item.clientName}}</view>
          </view>
        </view>
        <view class='work-item-foot'>{{item.date}}</view>
      </navigator>
      <!-- 到期提醒 -->
      <navigator wx:elif="{{item.workType === workType.WORK_DQ}}" class='work-item app-shadow-3 work-dqtx'>
        <view class='work-item-head'>
          到期提醒
        </view>
        <view class='work-item-body'>
          <view class='work-item-body-title'>
            <view class='work-item-headimg'><image src='{{item.clientImg}}'></image></view>
            <view class='work-item-name'>{{item.clientName}}</view>
          </view>
          <view class='work-item-body-con'>
            {{item.data.conText}}
          </view>
        </view>
        <view class='work-item-foot'>{{item.date}}</view>
      </navigator>
    </view>
  </view>
</view>