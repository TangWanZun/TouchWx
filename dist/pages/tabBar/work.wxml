<view class="page-body">
  <view class='work-type-list app-scroll-none'>
    <navigator url='{{item.url}}' class='work-type' wx:for="{{workTypeData}}" wx:key="{{item.title}}">
      <view class='work-type-icon'><image src='{{item.iconUrl}}'></image></view>
      <view  class='work-type-title'>{{item.title}}</view>
    </navigator>
  </view>
  <view class='work-list-title'>
    今日工作
  </view>
  <view class='work-list'>
    <view class='work-item-box' wx:for="{{dataList}}" wx:key="{{item.id}}">
      <!-- 救援服务 -->
      <navigator wx:if="{{item.DocType === workType.WORK_JY}}"  class='work-item  work-jyfw'>
        <view class='work-item-head'>
          紧急救援
        </view>
        <view class='work-item-body'>
          <view class='work-item-body-title'>
            <view class='work-item-headimg'><image src='{{imgUrl+item.ProfilePhoto}}'></image></view>
            <view class='work-item-name'>{{item.CardName}}</view>
          </view>
          <view class='work-item-car'>{{item.CarNum}}</view>
          <view class='work-item-body-con'>
            <span><image src='/assets/icon/dz.svg'></image></span>{{item.Note}}
          </view>
        </view>
        <view class='work-item-foot'>
          {{item.CreateDate}}
        </view>
      </navigator>
      <!-- 投诉建议 -->
      <navigator wx:elif="{{item.DocType === workType.WORK_TS}}" class='work-item  work-tsjy'>
        <view class='work-item-head'>
          投诉建议
        </view>
        <view class='work-item-body'>
          <view class='work-item-body-title'>
            <view class='work-item-headimg'><image src='{{imgUrl+item.ProfilePhoto}}'></image></view>
            <view class='work-item-name'>{{item.CardName}}</view>
          </view>
          <view class='work-item-body-con'>
            {{item.Note}}
          </view>
        </view>
        <view class='work-item-foot'>
          {{item.CreateDate}}
        </view>
      </navigator>
      <!-- 服务预约 -->
      <navigator wx:elif="{{item.DocType === workType.WORK_YY}}" class='work-item work-fwyy'>
        <view class='work-item-head'>
          {{item.Note}}
        </view>
        <view class='work-item-body'>
          <view class='work-item-body-con'>
            <view class='work-item-headimg'><image src='{{imgUrl+item.ProfilePhoto}}'></image></view>
            <view class='work-item-name'>{{item.CardName}}</view>
          </view>
          <view class='work-item-body-title'>
            {{item.CarNum}}
          </view>
        </view>
        <view class='work-item-foot'>
          <view>{{item.CreateDate}}</view>
          <view>{{item.ReserveStartDate}} - {{item.ReserveEndDate}}</view>
        </view>
      </navigator>
      <!-- 活动报名 -->
      <navigator wx:elif="{{item.DocType === workType.WORK_HD}}" class='work-item  work-hdbm'>
        <view class='work-item-head'>
          <view class='work-item-title'>{{item.Note}}</view>
          <view class='work-item-bg'><image mode="aspectFill" src='{{imgUrl+item.Img}}'></image></view>
        </view>
        <view class='work-item-body'>
          <view class='work-item-body-title'>活动报名</view>
          <view class='work-item-body-con'>
            <view class='work-item-headimg'><image src='{{imgUrl+item.ProfilePhoto}}'></image></view>
            <view class='work-item-name'>{{item.CardName}}</view>
          </view>
        </view>
        <view class='work-item-foot'>{{item.CreateDate}}</view>
      </navigator>
      <!-- 到期提醒 -->
      <navigator wx:elif="{{item.DocType === workType.WORK_DQ}}" class='work-item  work-dqtx'>
        <view class='work-item-head'>
          到期提醒
        </view>
        <view class='work-item-body'>
          <view class='work-item-body-title'>
            <view class='work-item-headimg'><image src='{{imgUrl+item.ProfilePhoto}}'></image></view>
            <view class='work-item-name'>{{item.CardName}}</view>
          </view>
          <view class='work-item-body-con'>
            {{item.Note}}
          </view>
        </view>
        <view class='work-item-foot'>{{item.CreateDate}}</view>
      </navigator>
    </view>
  </view>
</view>